type Calc{
    head:
        void main = task([string] args);
        int add = task(int a, b);

    body:
        void main = task([string] args){
            add(5, 3) -> print();

            add = task(int a, b){
                return a-b;
            }

            add(5, 3) -> print();
        }

        int add = task(int a, b){
            return a+b;
        }
}